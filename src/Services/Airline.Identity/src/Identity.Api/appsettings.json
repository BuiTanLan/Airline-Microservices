{
  "AppOptions": {
    "Name": "Identity-Service"
  },
  "LoggerOptions": {
    "level": "Information",
    "LogTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level} - {Message:lj}{NewLine}{Exception}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\sqlexpress;Database=IdentityDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "RabbitMq": {
    "HostName": "localhost",
    "ExchangeName": "identity",
    "UserName": "guest",
    "Password": "guest"
  },
  "OutboxOptions": {
    "Enabled": true,
    "UseBackgroundDispatcher": true
  },
  "AllowedHosts": "*"
}
