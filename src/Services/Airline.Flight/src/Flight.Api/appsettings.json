{
  "AppOptions": {
    "Name": "Flight-Service"
  },
  "LoggerOptions": {
    "level": "Information",
    "LogTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level} - {Message:lj}{NewLine}{Exception}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\sqlexpress;Database=FlightDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Jwt": {
    "Authority": "https://localhost:5005",
    "Audience": "flight-api"
  },
  "RabbitMq": {
    "HostName": "localhost",
    "ExchangeName": "flight",
    "UserName": "guest",
    "Password": "guest"
  },
  "OutboxOptions": {
    "Enabled": true,
    "UseBackgroundDispatcher": true
  },
  "InternalProcessorOptions": {
    "Enabled": true,
    "UseBackgroundDispatcher": true
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2",
      "Url": "https://localhost:5003"
    }
  },
  "AllowedHosts": "*"
}
